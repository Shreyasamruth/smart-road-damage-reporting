{
    "version": 2,
    "buildCommand": "cd smart-road-damage-reporting-main/frontend && npm install && npm run build",
    "outputDirectory": "smart-road-damage-reporting-main/frontend/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}